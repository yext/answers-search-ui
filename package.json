{
  "name": "answers",
  "version": "1.0.0",
  "description": "Javascript Answers Programming Interface",
  "main": "gulpfile.js",
  "dependencies": {
    "handlebars": "^4.1.2",
    "template-helpers": "^1.0.1"
  },
  "devDependencies": {
    "@babel/core": "^7.3.4",
    "@babel/preset-env": "^7.3.4",
    "@types/jest": "^24.0.15",
    "autoprefixer": "7.2.5",
    "cssnano": "^4.1.10",
    "gulp": "^4.0.1",
    "gulp-concat": "^2.6.1",
    "gulp-declare": "^0.3.0",
    "gulp-handlebars": "^5.0.2",
    "gulp-postcss": "^8.0.0",
    "gulp-remove-code": "^3.0.4",
    "gulp-rename": "^1.4.0",
    "gulp-rollup-lightweight": "^1.0.10",
    "gulp-sass": "^4.0.2",
    "gulp-uglify-es": "^1.0.4",
    "gulp-umd": "^2.0.0",
    "gulp-wrap": "^0.14.0",
    "jest": "^24.8.0",
    "jsdoc": "^3.6.3",
    "node-sass": "^4.12.0",
    "postcss-line-height-px-to-unitless": "0.2.1",
    "postcss-pxtorem": "4.0.1",
    "rollup": "^1.4.1",
    "rollup-plugin-babel": "^4.3.2",
    "rollup-plugin-commonjs": "^9.2.1",
    "rollup-plugin-node-builtins": "^2.1.2",
    "rollup-plugin-node-resolve": "^4.0.1",
    "semistandard": "^13.0.1",
    "size-limit": "^0.21.1",
    "vinyl-buffer": "^1.0.1",
    "vinyl-source-stream": "^2.0.0"
  },
  "scripts": {
    "build": "gulp && size-limit",
    "dev": "gulp dev",
    "docs": "jsdoc -R README.md -d docs/ -r src/",
    "lint": "semistandard",
    "test": "semistandard && jest",
    "size": "size-limit",
    "fix": "semistandard --fix",
    "install": "npm run build"
  },
  "jest": {
    "bail": 0,
    "verbose": true,
    "testMatch": [
      "**/tests/**/*.js"
    ]
  },
  "semistandard": {
    "globals": [
      "beforeEach",
      "describe",
      "it",
      "expect",
      "jest",
      "DOMParser"
    ],
    "ignore": [
      "docs/"
    ]
  },
  "author": "Billy",
  "license": "ISC"
}
